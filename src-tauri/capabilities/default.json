{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for app windows",
  "windows": ["main", "window-*"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:read-all",
    "fs:write-all"
  ]
}
